// Some example tasks for common languages.
//
// For more documentation on how to configure debug tasks,
// see: https://zed.dev/docs/debugger
[
  {
    "adapter": "Delve",
    "label": "Go: Test Function",
    "request": "launch",
    "mode": "test",
    "program": ".",
    "cwd": "$ZED_DIRNAME",
    "args": ["-test.run", "^$ZED_SYMBOL$", "-test.v"],
    "buildFlags": ["-tags=integration,slow"],
    "env": {}
  }
]
